{
	"$schema" : "http://json-schema.org/draft-04/schema#",
	"title" : "Google Play Top",
	"type" : "object",
	"properties" : {
		"rating" : {
			"type" : "number",
			"minimum" : 0,
			"maximum" : 5
		},
		"image" : {
			"type" : "string",
			"format" : "uri"
		},
		"rating_in_percentage" : {
			"type" : "number",
			"minimum" : 0,
			"maximum" : 100
		},
		"id" : {
			"type" : "string",
			"pattern" : "^[a-zA-Z0-9\\_\\.]+$"
		},
		"position" : {
			"type" : "integer",
			"minimum" : 1
		},
		"description" : {
			"type" : "string"
		},
		"category" : {
			"type" : "string",
			"pattern" : "^[A-Z0-9\\_]+$"
		},
		"url" : {
			"type" : "string",
			"format" : "uri"
		},
		"price" : {
			"type" : "string",
			"pattern" : "^\\d+,\\d{2}\\D+"
		},
		"name" : {
			"type" : "string"
		},
		"producer" : {
			"type" : "string"
		}
	},
	"required" : [
		"rating",
		"image",
		"rating_in_percentage",
		"id",
		"position",
		"category",
		"url",
		"name",
		"producer"
	]
}
