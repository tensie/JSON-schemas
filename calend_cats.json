{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "список праздников данной категории",
  "type": "object",
  "properties": {
    "id": {
      "description": "идентификатор категории",
      "type": "string"
    },
    "title": {
      "description": "название категории",
      "type": "string"
    },
    "holidays": {
      "description": "список id праздников",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "id",
    "title",
    "holidays"
  ]
}